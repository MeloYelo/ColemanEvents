@model WGRanch.ViewModels.WaterRightUpdateViewModel

@{
	ViewBag.Title = "Update Water Right";
}

@using (Html.BeginForm(null, null, FormMethod.Post, new { @class = "form-horizontal" }))
{
	@Html.ValidationSummary()
	<fieldset class="form">
		<legend>Update Water Right</legend>
		@Html.Partial("_CreateUpdate", Model.WaterRight)
		<div class="control-group">
			<div class="controls">
				<button type="submit" class="btn btn-primary">Apply Changes </button>
			</div>
		</div>
	</fieldset>
}
