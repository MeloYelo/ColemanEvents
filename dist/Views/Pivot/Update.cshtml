@model WGRanch.Models.Pivot

@{
	ViewBag.Title = "Update Pivot";
}

@using (Html.BeginForm())
{
	@Html.ValidationSummary()
	<fieldset class="form">
		<legend>Update Pivot</legend>
		@Html.Partial("_CreateUpdate", Model)
		<br />
		<button class="button" type="submit">
			Apply Changes
		</button>
	</fieldset>
}